{
  "name": "erp-template-vuetify",
  "version": "0.0.1",
  "template": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:expose": "vite --host",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore tests",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --build --force",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "build:uat": "run-p type-check \"build-only:uat {@}\" --",
    "build-only:uat": "vite build --mode uat",
    "build:prod": "run-p type-check \"build-only:prod {@}\" --",
    "build-only:prod": "vite build --mode production"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "axios": "1.7.4",
    "@siloamhospitals/erp-template-vuetify-components": "2.0.2",
    "encrypt-storage": "^2.13.3",
    "pinia": "2.2.1",
    "vee-validate": "^4.13.2",
    "vue": "3.4.37",
    "vue-router": "4.4.3",
    "vuetify": "3.6.14",
    "yup": "^1.4.0",
    "vue3-promise-dialog": "^0.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.4",
    "@tsconfig/node20": "20.1.4",
    "@types/jsdom": "21.1.7",
    "@types/node": "20.14.14",
    "@vitejs/plugin-vue": "5.1.2",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.57.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-vue": "9.27.0",
    "jsdom": "24.1.1",
    "npm-run-all2": "6.2.2",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "typescript": "5.5.4",
    "unplugin-fonts": "1.1.1",
    "unplugin-vue-components": "0.27.3",
    "vite": "5.4.0",
    "vite-plugin-vuetify": "2.0.4",
    "vitest": "1.6.0",
    "vue-tsc": "2.0.29"
  }
}
